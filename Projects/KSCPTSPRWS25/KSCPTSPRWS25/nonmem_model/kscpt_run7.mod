$PROBLEM  SigmoidEmaxPD

$INPUT  ID	AUC_glu	PG_avg	PG_base	HbA1c_base	N_DAY	UGE24	VOLUME24	UGEbase	VOLUMEbase	GRP	AGE	SEX	HT	WT	BMI	ALB	eGFR	TBIL	SODIUM	AST	ALT	UID=DROP	dUGE	dUGEc	EFFECT1=DV

$DATA    ..//KSCPTSPRWS25_SGLT2i_PD.csv IGNORE=@

$PRED
E0    = THETA(1)
EMAX  = THETA(2) * EXP(ETA(1))
EC50  = THETA(3) * EXP(ETA(2))
HILL  = THETA(4)

IPRED = E0 + EMAX * eGFR**HILL / (EC50**HILL + eGFR**HILL)
IRES = DV - IPRED
IWRES = IRES / IPRED
Y = IPRED + EPS(1)

$THETA
(0, 0.03) ; E0
(0, 0.5)  ; EMAX
(0, 50)   ; EC50
(0, 7)    ; HILL

$OMEGA
0.02 ; IIV on EMAX
0.02 ; IIV on EC50


$SIGMA
0.01 ; 

$ESTIMATION METHOD=1 INTER MAXEVAL=9999 PRINT=5
$COVARIANCE
$TABLE ID eGFR dUGEc DV IPRED ETA1 ETA2 WRES CWRES FILE=sdtab007 NOPRINT ONEHEADER
$TABLE E0 EMAX EC50 HILL ONEHEADER NOPRINT  FIRSTONLY FILE=patab007
